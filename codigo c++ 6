//Saber la edad promedio a la que se graduan estudiantes de lic en fisica
//Diana Laura de la vega Trinidad
//30-10-17
 #include <iostream>
using namespace std;
int main () {
    int edad[14];
    int suma=0;

for (int i=0; i<14; i++)
    {           
    cout << "Â¿A que edad te piensas graduar?"<<endl;
    cin>>edad[i];
    suma+=edad[i];
}

for (int j=0; j<14; j++)
    cout<< edad[j] <<"j";
cout<<"La edad promedio de graduacion esperada es"<<suma/14<<endl;

return 0;
    
}

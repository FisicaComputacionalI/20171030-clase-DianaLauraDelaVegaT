//Saber la edad promedio a la que se graduan estudiantes de lic en fisica
//Diana Laura de la vega Trinidad
//30-10-17
 #include <iostream>
using namespace std;
int main () {
    size_t talla=14;
    int *edad=new int[talla];
    int suma=0;

for (int i=0; i<talla; i++)
    {           
    cout << "Â¿A que edad te piensas graduar?"<<endl;
    cin>>edad[i];
    suma+=edad[i];
}

for (int j=0; j<talla; j++)
    cout<< edad[j] <<"j";
cout<<"La edad promedio de graduacion esperada es"<<suma/talla<<endl;
delete [] edad;
return 0;
    
}
